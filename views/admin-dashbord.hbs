{{> admin-navbar}}
<div class="container">
   <div class="row mt-5">
      <div class="col-12">
         <h3 class="text-center">Usears</h3>
         <table class="table table-dark table-striped table-responsive-md">
            <thead class="bg-info">
               <tr>
                  <th scope="col">Sl</th>
                  <th scope="col">Name</th>
                  <th scope="col">place</th>
                  <th scope="col">gender</th>
                  <th scope="col">edit</th>
                  <th scope="col">View</th>
               </tr>
            </thead>
            <tbody>
               {{#each userList }}
               <tr>
                  <th scope="row">1</th>
                  <td> {{this.name}} </td>
                  <td> {{ this.address }} </td>
                  <td> {{ this.gender }} </td>
                  <td> {{ editLink }} </td>
                  <td> {{ ViewLink }} </td>
               </tr>
               {{/each }}
            </tbody>
         </table>
      </div>
   </div>
   <div class="row justify-content-end">
      <div class="mr-5">
         <a href="/admin/add-user" class="btn btn-info"> Add user</a>
      </div>
   </div>
</div>
